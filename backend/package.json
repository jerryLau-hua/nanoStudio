{
    "name": "nano-studio-backend",
    "version": "1.0.1",
    "description": "Backend API for Nano Studio - A modern knowledge management and code analysis platform",
    "main": "dist/app.js",
    "scripts": {
        "dev": "nodemon src/app.ts",
        "build": "tsc",
        "start": "node dist/app.js",
        "test": "jest",
        "lint": "eslint . --ext .ts",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:migrate": "prisma migrate dev",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:studio": "prisma studio",
        "db:seed": "ts-node prisma/seed.ts"
    },
    "keywords": [
        "api",
        "express",
        "typescript",
        "notebook",
        "code-analysis"
    ],
    "author": "JerryLau",
    "license": "MIT",
    "dependencies": {
        "@prisma/client": "^5.22.0",
        "@qdrant/js-client-rest": "^1.16.2",
        "@types/minio": "^7.1.1",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "helmet": "^7.1.0",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "minio": "^8.0.6",
        "multer": "^2.0.2",
        "pdf-parse": "^2.4.5",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^2.0.0",
        "@types/node": "^20.10.5",
        "@types/pdf-parse": "^1.1.5",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prisma": "^5.22.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
